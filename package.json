  "description": "MCP Server for remote OpenCode HTTP API - Connect OpenClaw to OpenCode agents",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "opencode-mcp-server": "./dist/index.js"
  },
  "scripts": {
