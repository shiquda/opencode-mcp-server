  "author": "shiquda",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/shiquda/opencode-mcp-server.git"
  },
  "bugs": {
    "url": "https://github.com/shiquda/opencode-mcp-server/issues"
  },
  "homepage": "https://github.com/shiquda/opencode-mcp-server#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.4",
    "express": "^4.18.2",
    "node-fetch": "^3.3.2",
